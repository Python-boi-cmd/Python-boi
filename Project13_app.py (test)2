#streamlit test
import streamlit as st

st.set_page_config(page_title="Math Quiz", page_icon=":small_airplane:", layout="wide")

import random

x = random.randint(0,10)
y = random.randint(0,10)

st.write(f"What is {x} * {y}?")
x = st.number_input("Enter here: ")
if (x) == (x) * (y):
    st.write("Correct.")
else:
    print("Wrong.")
